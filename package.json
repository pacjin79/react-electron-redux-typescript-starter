{
  "name": "news_feeder",
  "version": "1.0.0",
  "description": "Electron react news app",
  "main": "src/index.tsx",
  "scripts": {
    "build": "NODE_ENV=production node_modules/webpack/bin/webpack.js --config config/webpack/prod.js --progress --profile --colors",
    "startDev": "cross-env NODE_ENV=development node $NODE_DEBUG_OPTION devServer.js",
    "startHot": "cross-env HOT=1 NODE_ENV=development electron ./src/app.js",
